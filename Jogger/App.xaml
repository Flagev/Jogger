<Application x:Class="Jogger.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Jogger"
             xmlns:vm="clr-namespace:Jogger.ViewModels" 
             xmlns:sys="clr-namespace:System;assembly=System.Runtime">
    <Application.Resources>
        <vm:ViewModelLocator
            xmlns:vm="clr-namespace:Jogger.ViewModels" 
            x:Key="Locator" />
        <sys:String x:Key="ApplicationTitle">KA Tester</sys:String>
        <sys:String x:Key="Author">Michał Sobczak</sys:String>
        <sys:String x:Key="Version">v0.1</sys:String>
        <sys:String x:Key="CopyRight">Kongsberg Automotive</sys:String>
        <sys:String x:Key="HelpTitle">Pomoc</sys:String>
        <sys:String x:Key="HelpText">Aplikacja do testowania zaworów</sys:String>
        <sys:String x:Key="AboutTitle">Informacje o aplikacji</sys:String>
        <x:Array x:Key="ComboBoxItems" Type="sys:String">
            <sys:String>Item1</sys:String>
            <sys:String>Item2</sys:String>
            <sys:String>Item3</sys:String>
        </x:Array>
    </Application.Resources>
</Application>
